{
  "name": "database",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "ts-lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "precommit": "lint-staged",
    "generate": "prisma generate",
    "push": "prisma db push --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "5.6.0",
    "prisma": "5.6.0"
  },
  "devDependencies": {
    "eslint": "*",
    "lint-staged": "*",
    "typescript": "*"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext ts --ext tsx --fix"
    ]
  }
}
