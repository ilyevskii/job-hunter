{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "public/dist/**"],
      "dependsOn": ["^build"]
    },
    "database#generate": {
      "cache": false
    },
    "database#push": {
      "cache": false
    },
    "api#schedule-dev": {
      "dependsOn": ["database#generate"],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "development": {
      "dependsOn": ["api#schedule-dev", "dev"],
      "cache": false
    },
    "precommit": {
      "outputs": []
    }
  }
}
